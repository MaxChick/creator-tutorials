{
    "name": "cylinder",
    "alternateNames": [
      "tube",
      "poly cylinder"
    ],
    "defaultNodeName": "cylinder",
    "path": [
      "geometry",
      "create"
    ],
    "help": "Creates a cylinder between two vector positions.",
    "input": {
      "start": {
        "type": "flexi:vector",
        "displayName": "start",
        "default": [
          0,
          0,
          0
        ]
      },
      "end": {
        "type": "flexi:vector",
        "displayName": "end",
        "default": [
          0,
          0,
          1000
        ]
      },
      "radius": {
        "type": "flexi:float",
        "displayName": "radius",
        "default": 500
      },
      "segments": {
        "type": "flexi:integer",
        "displayName": "segments",
        "default": 16,
        "minimum": 3,
        "maximum": 100
      }
    },
    "type": "PolyCylinder",
    "output": {
      "geometry": {
        "type": "geometry",
        "displayName": "geometry"
      },
      "points": {
        "type": "list:vector",
        "displayName": "points",
        "description": "Point positions."
      },
      "points.x": {
        "type": "list:float",
        "displayName": "points.x",
        "description": "Point x values."
      },
      "points.y": {
        "type": "list:float",
        "displayName": "points.y",
        "description": "Point y values."
      },
      "points.z": {
        "type": "list:float",
        "displayName": "points.z",
        "description": "Point z values."
      }
    },
    "nodeClass": "node"
  }