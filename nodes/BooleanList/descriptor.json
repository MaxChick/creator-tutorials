{
    "name": "boolean list",
    "alternateNames": [
      "bitmask",
      "mask"
    ],
    "defaultNodeName": "boolean list",
    "path": [
      "value",
      "list"
    ],
    "help": "Creates a list of boolean values.",
    "input": {
      "list": {
        "type": "list:boolean",
        "displayName": "list"
      },
      "size": {
        "type": "integer",
        "displayName": "size",
        "default": 10
      },
      "defaultValue": {
        "type": "boolean",
        "displayName": "default",
        "default": false
      },
      "pattern": {
        "type": "boolean",
        "displayName": "pattern",
        "default": false,
        "description": "Set a pattern of boolean values."
      },
      "start": {
        "type": "integer",
        "displayName": "start",
        "default": 0,
        "hideWhen": "!pattern"
      },
      "end": {
        "type": "integer",
        "displayName": "end",
        "default": 10,
        "hideWhen": "!pattern"
      },
      "skip": {
        "type": "integer",
        "displayName": "skip",
        "default": 1,
        "hideWhen": "!pattern"
      },
      "every": {
        "type": "integer",
        "displayName": "every",
        "default": 3,
        "hideWhen": "!pattern"
      },
      "initialise": {
        "type": "boolean",
        "displayName": "initialize",
        "default": false,
        "description": "Sets the list input to the specified size and its values to the default."
      }
    },
    "type": "BooleanList",
    "output": {
      "list": {
        "type": "list:boolean",
        "displayName": "list"
      }
    },
    "nodeClass": "value"
  }