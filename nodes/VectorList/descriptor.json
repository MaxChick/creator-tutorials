{
    "name": "vector list",
    "alternateNames": [
      "points",
      "coordinates",
      "directions",
      "xyz"
    ],
    "defaultNodeName": "vector list",
    "path": [
      "value",
      "list"
    ],
    "help": "Creates a list of 3 component xyz vector values.",
    "input": {
      "list": {
        "type": "list:vector",
        "displayName": "list"
      },
      "size": {
        "type": "integer",
        "displayName": "size",
        "default": 10
      },
      "defaultValue": {
        "type": "vector",
        "displayName": "default",
        "default": [
            0,
            0,
            0
          ]
      },
      "initialise": {
        "type": "boolean",
        "displayName": "initialize",
        "default": false,
        "description": "Sets the list input to the specified size and its values to the default."
      }
    },
    "type": "VectorList",
    "output": {
      "list": {
        "type": "list:vector",
        "displayName": "list"
      }
    },
    "nodeClass": "value"
  }