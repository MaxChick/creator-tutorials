{
    "name": "number list",
    "alternateNames": [
      "float",
      "decimal"
    ],
    "defaultNodeName": "number list",
    "path": [
      "value",
      "list"
    ],
    "help": "Creates a list of number/float values.",
    "input": {
      "list": {
        "type": "list:float",
        "displayName": "list"
      },
      "size": {
        "type": "integer",
        "displayName": "size",
        "default": 10
      },
      "defaultValue": {
        "type": "float",
        "displayName": "default",
        "default": 0
      },
      "initialise": {
        "type": "boolean",
        "displayName": "initialize",
        "default": false,
        "description": "Sets the list input to the specified size and its values to the default."
      }
    },
    "type": "FloatList",
    "output": {
      "list": {
        "type": "list:float",
        "displayName": "list"
      }
    },
    "nodeClass": "value"
}