{
    "name": "vector",
    "alternateNames": [
      "point",
      "coordinate",
      "direction",
      "xyz"
    ],
    "defaultNodeName": "vector",
    "path": [
      "value",
      "parameter"
    ],
    "help": "Creates an xyz vector value.",
    "input": {
      "value": {
        "type": "vector",
        "displayName": "value"
      },
      "defaultValue": {
        "type": "vector",
        "displayName": "default"
      },
      "setToDefault": {
        "type": "button",
        "displayName": "set to default"
      },
      "hideControl": {
        "type": "boolean",
        "displayName": "hide control",
        "default": false
      },
      "hideLabel": {
        "type": "boolean",
        "displayName": "hide label",
        "default": false,
        "hideWhen": "hideControl"
      }
    },
    "type": "VectorValue",
    "output": {
      "value": {
        "type": "vector",
        "displayName": "value",
        "defaultOutput": true
      },
      "x": {
        "type": "float",
        "displayName": "x",
        "description": "x value of vector."
      },
      "y": {
        "type": "float",
        "displayName": "y",
        "description": "y value of vector."
      },
      "z": {
        "type": "float",
        "displayName": "z",
        "description": "z value of vector."
      }
    },
    "nodeClass": "value"
}